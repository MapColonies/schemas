{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://mapcolonies.com/common/s3/partial/v1",
  "type": "object",
  "properties": {
    "accessKeyId": {
      "type": "string",
      "description": "The access key id of the bucket",
      "x-populate-as-env": "AWS_ACCESS_KEY_ID"
    },
    "secretAccessKey": {
      "type": "string",
      "description": "The secret access key of the bucket",
      "x-populate-as-env": "AWS_SECRET_ACCESS_KEY"
    }
  }
}
